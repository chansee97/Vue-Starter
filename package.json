{
  "name": "enchadmin",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev ",
    "dev:test": "vite --mode test ",
    "dev:prod": "vite --mode prod ",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "stylelint:fix": "npx stylelint *.{css,scss,vue,less,html} --fix"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,less,html}": [
      "stylelint --fix"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@types/node": "^20.3.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "eslint": "^8.38.0",
    "lint-staged": "^13.2.2",
    "naive-ui": "^2.34.4",
    "rollup-plugin-visualizer": "^5.9.2",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.7.0",
    "typescript": "^5.1.3",
    "unocss": "^0.53.1",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^1.6.5"
  }
}
