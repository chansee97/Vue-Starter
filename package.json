{
  "name": "virtuoso",
  "type": "module",
  "version": "0.1.0",
  "author": "Rock Chen",
  "license": "MIT",
  "homepage": "https://github.com/chansee97/virtuoso",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chansee97/virtuoso.git"
  },
  "bugs": {
    "url": "https://github.com/chansee97/virtuoso/issues"
  },
  "keywords": [
    "vitailse",
    "tailwindcss",
    "vite",
    "vitesse"
  ],
  "scripts": {
    "dev": "vite --mode dev --port 5200 --open",
    "dev:test": "vite --mode test ",
    "dev:prod": "vite --mode prod ",
    "build": "vite build --mode prod",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylelint:fix": "npx stylelint *.{css,scss,vue,less,html} --fix",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.5",
    "@vueuse/core": "^10.8.0",
    "axios": "^1.6.7",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.20",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@iconify-json/icon-park-outline": "^1.1.15",
    "@types/node": "^20.11.20",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.38.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.8.4",
    "vite": "^5.1.4",
    "vite-bundle-visualizer": "^1.0.1",
    "vite-plugin-pwa": "^0.19.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
